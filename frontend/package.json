{
  "name": "simple-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start-pm2-dev": "pm2 restart pm2.dev.config.js --update-env",
    "start-pm2-prod": "pm2 restart pm2.prod.config.js --update-env",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "dev": "npm run format && npm run prisma:generate  && next dev",
    "prisma:generate": "npx prisma generate",
    "build": "npm run format && npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@auth/core": "^0.19.0",
    "@auth/prisma-adapter": "^1.0.12",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.15.0",
    "@uiw/react-md-editor": "^4.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "jose": "^5.2.0",
    "lucide-react": "^0.299.0",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.3",
    "socket.io-client": "^4.7.2",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
